<h1>ログイン機能</h1>

<%= form_with url: sessions_path do |form| %>
    <div class="form-group">
        <%= form.label :name, "名前" %>
        <%= form.text_field :name %>
    </div>
    <div class="form-group">
        <%= form.label :password, "パスワード" %>
        <%= form.text_field :password %>
    </div>
    <%= form.submit %>
<% end %>