<h1>在庫一覧</h1>

<table border="1">
    <tr>
        <th>商品名</th>
        <th>単価</th>
        <th>量</th>
        <th>操作</th>
    </tr>
        <% @stocks.each do |stock| %>
    <tr>
        <td><%= stock.productName %></td>
        <td><%= stock.price %></td>
        <td><%= stock.quantity %></td>
        <td><%= link_to "詳細" %></td>
    </tr>
        <% end %>
</table>