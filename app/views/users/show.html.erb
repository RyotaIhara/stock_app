<p id="notice"><%= notice %></p>
<h1 class="label">会員の詳細</h1>

<%= link_to '編集', edit_user_path(@user) %> |
<%= link_to "会員一覧へ", :users %>

<table class="table">
    <tr>
        <th width="150">ユーザー名</th>
        <td><%= @user.name %></td>
    </tr>
    <tr>
        <th>メールアドレス</th>
        <td><%= @user.email %></td>
    </tr>
    <tr>
        <th>管理者</th>
        <td><%= @user.administrator? ? "◯" : "-" %></td>
    </tr>
</table>